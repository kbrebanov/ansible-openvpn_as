{
  "Default": {
    "admin_ui.https.ip_address": "{{ openvpn_as_admin_ui_https_ip_address }}",
    "admin_ui.https.port": "{{ openvpn_as_admin_ui_https_port }}",
    "auth.ldap.0.name": "{{ openvpn_as_auth_ldap_host }}",
    "auth.ldap.0.ssl_verify": "never",
    "auth.ldap.0.timeout": "4",
    "auth.ldap.0.use_ssl": "{{ openvpn_as_auth_ldap_use_ssl }}",
    "auth.ldap.0.server.0.host": "{{ openvpn_as_auth_ldap_host }}",
    "auth.ldap.0.uname_attr": "{{ openvpn_as_auth_ldap_uname_attr }}",
    "auth.ldap.0.users_base_dn": "{{ openvpn_as_auth_ldap_users_base_dn }}",
{% if openvpn_as_auth_ldap_bind_dn %}
    "auth.ldap.0.bind_dn": "{{ openvpn_as_auth_ldap_bind_dn }}",
    "auth.ldap.0.bind_pw": "{{ openvpn_as_auth_ldap_bind_pw }}",
{% endif %}
    "auth.module.type": "{{ openvpn_as_auth_module_type }}",
    "auth.pam.0.service": "openvpnas",
    "auth.radius.0.acct_enable": "false",
    "auth.radius.0.name": "My Radius servers",
{% if openvpn_as_cs_admin_only %}
    "cs.admin_only": "true",
{% else %}
    "cs.admin_only": "false",
{% endif %}
    "cs.cws_proto_v2": "true",
{% if openvpn_as_cs_cws_ui_offer_android %}
    "cs.cws_ui_offer.android": "true",
{% else %}
    "cs.cws_ui_offer.android": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_autologin %}
    "cs.cws_ui_offer.autologin": "true",
{% else %}
    "cs.cws_ui_offer.autologin": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_ios %}
    "cs.cws_ui_offer.ios": "true",
{% else %}
    "cs.cws_ui_offer.ios": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_linux %}
    "cs.cws_ui_offer.linux": "true",
{% else %}
    "cs.cws_ui_offer.linux": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_mac %}
    "cs.cws_ui_offer.mac": "true",
{% else %}
    "cs.cws_ui_offer.mac": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_server_locked %}
    "cs.cws_ui_offer.server_locked": "true",
{% else %}
    "cs.cws_ui_offer.server_locked": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_user_locked %}
    "cs.cws_ui_offer.user_locked": "true",
{% else %}
    "cs.cws_ui_offer.user_locked": "false",
{% endif %}
{% if openvpn_as_cs_cws_ui_offer_win %}
    "cs.cws_ui_offer.win": "true",
{% else %}
    "cs.cws_ui_offer.win": "false",
{% endif %}
    "cs.https.ip_address": "{{ openvpn_as_cs_https_ip_address }}",
    "cs.https.port": "{{ openvpn_as_cs_https_port }}",
    "cs.prof_sign_web": "true",
    "cs.ssl_method": "SSLv3",
{% if openvpn_as_cs_ssl_reneg %}
    "cs.ssl_reneg": "true",
{% else %}
    "cs.ssl_reneg": "false",
{% endif %}
    "cs.tls_version_min": "{{ openvpn_as_cs_tls_version_min }}",
    "host.name": "{{ openvpn_as_host_name }}",
{% if openvpn_as_iptables_append %}
    "iptables.append": "true",
{% else %}
    "iptables.append": "false",
{% endif %}
{% if openvpn_as_iptables_vpn_disable_filter %}
    "iptables.vpn.disable.filter": "true",
{% else %}
    "iptables.vpn.disable.filter": "false",
{% endif %}
{% if openvpn_as_iptables_vpn_disable_mangle %}
    "iptables.vpn.disable.mangle": "true",
{% else %}
    "iptables.vpn.disable.mangle": "false",
{% endif %}
{% if openvpn_as_iptables_vpn_disable_nat %}
    "iptables.vpn.disable.nat": "true",
{% else %}
    "iptables.vpn.disable.nat": "false",
{% endif %}
    "sa.initial_run_groups.0": "web_group",
    "sa.initial_run_groups.1": "openvpn_group",
    "sa.ssl_lib": "{{ openvpn_as_sa_ssl_lib }}",
{% if openvpn_as_vpn_client_config_directives %}
    "vpn.client.config_text": "{{ openvpn_as_vpn_client_config_directives | join('\n') }}",
{% else %}
    "vpn.client.config_text": "",
{% endif %}
{% if openvpn_as_vpn_client_routing_inter_client %}
    "vpn.client.routing.inter_client": "true",
{% else %}
    "vpn.client.routing.inter_client": "false",
{% endif %}
{% if openvpn_as_vpn_client_routing_reroute_dns and (openvpn_as_vpn_server_dhcp_option_dns_0 != '' or openvpn_as_vpn_server_dhcp_option_dns_1 != '') %}
    "vpn.client.routing.reroute_dns": "custom",
{% elif openvpn_as_vpn_client_routing_reroute_dns %}
    "vpn.client.routing.reroute_dns": "true",
{% else %}
    "vpn.client.routing.reroute_dns": "false",
{% endif %}
{% if openvpn_as_vpn_client_routing_reroute_gw %}
    "vpn.client.routing.reroute_gw": "true",
{% else %}
    "vpn.client.routing.reroute_gw": "false",
{% endif %}
{% if openvpn_as_vpn_client_routing_superuser_c2c_access %}
    "vpn.client.routing.superuser_c2c_access": "true",
{% else %}
    "vpn.client.routing.superuser_c2c_access": "false",
{% endif %}
    "vpn.general.osi_layer": "{{ openvpn_as_vpn_general_osi_layer }}",
{% for daemon in openvpn_as_vpn_daemons %}
    "vpn.daemon.{{ loop.index0 }}.client.netmask_bits": "{{ daemon.client_netmask_bits }}",
    "vpn.daemon.{{ loop.index0 }}.client.network": "{{ daemon.client_network }}",
    "vpn.daemon.{{ loop.index0 }}.listen.ip_address": "{{ daemon.listen_ip_address }}",
    "vpn.daemon.{{ loop.index0 }}.listen.port": "{{ daemon.listen_port }}",
    "vpn.daemon.{{ loop.index0 }}.listen.protocol": "{{ daemon.listen_protocol }}",
    "vpn.daemon.{{ loop.index0 }}.server.ip_address": "{{ daemon.server_ip_address }}",
{% endfor %}
{% if openvpn_as_vpn_server_daemon_enable %}
    "vpn.server.daemon.enable": "true",
{% else %}
    "vpn.server.daemon.enable": "false",
{% endif %}
    "vpn.server.daemon.tcp.n_daemons": "1",
    "vpn.server.daemon.tcp.port": "{{ openvpn_as_vpn_server_daemon_tcp_port }}",
    "vpn.server.daemon.udp.n_daemons": "1",
    "vpn.server.daemon.udp.port": "{{ openvpn_as_vpn_server_daemon_udp_port }}",
{% if openvpn_as_vpn_server_config_directives %}
    "vpn.server.config_text": "{{ openvpn_as_vpn_server_config_directives | join('\n') }}",
{% else %}
    "vpn.server.config_text": "",
{% endif %}
{% if openvpn_as_vpn_server_dhcp_option_adapter_domain_suffix != '' %}
    "vpn.server.dhcp_option.adapter_domain_suffix": "{{ openvpn_as_vpn_server_dhcp_option_adapter_domain_suffix }}",
{% endif %}
{% if openvpn_as_vpn_server_dhcp_option_dns_0 != '' %}
    "vpn.server.dhcp_option.dns.0": "{{ openvpn_as_vpn_server_dhcp_option_dns_0 }}",
{% endif %}
{% if openvpn_as_vpn_server_dhcp_option_dns_1 != '' %}
    "vpn.server.dhcp_option.dns.1": "{{ openvpn_as_vpn_server_dhcp_option_dns_1 }}",
{% endif %}
{% if openvpn_as_vpn_server_dhcp_option_domains %}
    "vpn.server.dhcp_option.domain": "{{ openvpn_as_vpn_server_dhcp_option_domains | join(',') }}"
{% endif %}
{% if openvpn_as_vpn_server_duplicate_cn %}
    "vpn.server.duplicate_cn": "true",
{% else %}
    "vpn.server.duplicate_cn": "false",
{% endif %}
{% if openvpn_as_vpn_server_google_auth_enable %}
    "vpn.server.google_auth.enable": "true",
{% else %}
    "vpn.server.google_auth.enable": "false",
{% endif %}
{% for pool in openvpn_as_vpn_server_group_pool %}
    "vpn.server.group_pool.{{ loop.index0 }}": "{{ pool }}",
{% endfor %}
    "vpn.server.port_share.enable": "true",
    "vpn.server.port_share.ip_address": "1.2.3.4",
    "vpn.server.port_share.port": "1234",
    "vpn.server.port_share.service": "admin+client",
{% if openvpn_as_vpn_server_routing_allow_private_nets_to_clients %}
    "vpn.server.routing.allow_private_nets_to_clients": "true",
{% else %}
    "vpn.server.routing.allow_private_nets_to_clients": "false",
{% endif %}
    "vpn.server.routing.gateway_access": "{{ openvpn_as_vpn_server_routing_gateway_access }}",
    "vpn.server.routing.private_access": "{{ openvpn_as_vpn_server_routing_private_access }}",
{% for network in openvpn_as_vpn_server_routing_private_networks %}
    "vpn.server.routing.private_network.{{ loop.index0 }}": "{{ network }}",
{% endfor %}
{% for subnet in openvpn_as_vpn_server_routing_routed_subnets %}
    "vpn.server.routing.routed_subnets.{{ loop.index0 }}": "{{ subnet }}",
{% endfor %}
{% if openvpn_as_vpn_server_static_network != '' %}
    "vpn.server.static.0.netmask_bits": "{{ openvpn_as_vpn_server_static_netmask_bits }}",
    "vpn.server.static.0.network": "{{ openvpn_as_vpn_server_static_network }}",
{% endif %}
{% if openvpn_as_vpn_server_tls_auth %}
    "vpn.server.tls_auth": "true",
{% else %}
    "vpn.server.tls_auth": "false",
{% endif %}
    "vpn.server.tls_version_min": "{{ openvpn_as_vpn_server_tls_version_min }}",
    "vpn.tls_refresh.do_reauth": "true",
    "vpn.tls_refresh.interval": "{{ openvpn_as_vpn_tls_refresh_interval }}",
    "xmlrpc.relay_level": "{{ openvpn_as_xmlrpc_relay_level }}"
  },
  "_INTERNAL": {
    "run_api.active_profile": "Default",
    "webui.edit_profile": "Default"
  }
}
